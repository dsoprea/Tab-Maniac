{
   "background": {
      "scripts": [ "js/jquery-1.7.2.min.js", 
                   "js/db.js", 
                   "js/patches.js", 
                   "js/ls.js", 
                   "js/extension.js" ]
   },
   "description": "Automatically backup your Chrome tabs.",
   "icons": {
      "128": "images/shield-128.png",
      "32": "images/shield-32.png",
      "48": "images/shield-48.png",
      "64": "images/shield-64.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "19",
   "name": "Tab Maniac",
   "options_page": "options.html",
   "permissions": [ "tabs", "unlimitedStorage" ],
   "update_url": "http://clients2.google.com/service/update2/crx",
   "version": "0.9.10"
}
